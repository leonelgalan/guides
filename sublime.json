{
	"binary_file_patterns":
	[
		"*.jpg",
		"*.jpeg",
		"*.png",
		"*.gif",
		"*.ttf",
		"*.tga",
		"*.dds",
		"*.ico",
		"*.eot",
		"*.pdf",
		"*.swf",
		"*.jar",
		"*.zip",
		"*.log",
		"tmp/*",
		"public/assets/*"
	],
	"draw_white_space": "all",
	"font_size": 15,
	"highlight_line": true,
	"ignored_packages":
	[
		"Vintage"
	],
	"rulers": [80,120],
	"tab_size": 2,
	"translate_tabs_to_spaces": true,
	"hot_exit": false,
	"remember_open_files": false,
	"folder_exclude_patterns": [".svn", ".git", ".hg", "CVS", "coverage"]
}
